v1 <- c(1,3,3)
v2 <- c(0,1,-1)
v3 <- c(1,1,1)
X = matrix(c(v1, v2, v3), nrow = 3, ncol = 3)
X
Xt = t(X)
X1 = Xt %*% X
X2 = solve(X1)
X3 = X2 %*% Xt
y = matrix(c(-1,1,1), nrow = 3, ncol = 1)
beta = X3 %*% y
beta
